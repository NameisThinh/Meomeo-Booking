@model IEnumerable<HTDL.Models.DichVu>


<div class="row p-2 m-0">
    @foreach (var item in Model)
    {
        <div class="col-md-3 p-3">
            <a href="/Home/DatKham/?MaDv=@item.MaDV">
                <div>
                    <img src="@item.HinhAnh" width="100%" height="150" />
                </div>
                <div class="text-uppercase" style="background-color: #efeef5">
                    @item.TenDV
                </div>
            </a>
        </div>

    }
</div>